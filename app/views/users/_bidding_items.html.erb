<h2>You have bid on ></h2>
<% unless @bids.empty? %>
<div class="table-box">
<%= will_paginate @bids %>
<table class="bidding-items">
  <thead>
    <tr>
      <th>Name</th>
      <th># bids</th>
    </tr>
  </thead>

  <tbody>
    <% @bids.each do |bid| %>
      <tr>
        <td><%= link_to bid.item.name, bid.item %></td>
        <td><%= bid.item.bids.count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<% else %>
  <p class="rule">No items</p>
<% end %>